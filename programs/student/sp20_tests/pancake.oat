}

void flip (int[] a, int p) {
  var t = 0;
  var s = 0;

  var pp = p;

  while (s < pp) {
    var b = a[s];
    t = b;

    var c = a[pp];
    a[s] = c;

    var f = t;
    a[pp] = f;

    var g = s + 1;
    s = g;

    var k = pp - 1;
    pp = k;
  }

  return;
}

int max (int[] a, int l) {
  var max_v = 0;

  for (var i = 0; i < l; i = i + 1;) {
    if (a[i] > a[max_v]) {
      var b = i;
      var c = b;
      max_v = c;
    }
  }

  return max_v;
}


void sort(int[] a, int l) {
  for (var c = l; c > 1; c = c - 1;) {
    var max_v = max(a, c);

    var d = c - 1;

    if (max_v != d) {
      flip(a, max_v);

      var e = c - 1;
      flip(a, e);
    }
  }

  return;
}

